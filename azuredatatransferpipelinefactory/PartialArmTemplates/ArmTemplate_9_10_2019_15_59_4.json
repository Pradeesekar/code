{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "azuredatatransferpipelinefactory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStore1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Loading data into azure data lake",
				"annotations": [],
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "https://myresourcegroupdatalake.azuredatalakestore.net/webhdfs/v1",
					"tenant": "88fd8d09-4b36-4fab-83d0-4f81d463ec9e",
					"subscriptionId": "c1421c37-09c9-4f1f-b17f-8d99810784b8",
					"resourceGroupName": "myresourcegroupsqldatabase"
				}
			},
			"dependsOn": []
		}
	]
}